{
  "name": "pokemon-tcg-search",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:6666": "vite --port 6666 --host 0.0.0.0",
    "dev:all": "bash start-dev.sh",
    "dev:all:win": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 vitest",
    "test:run": "NODE_OPTIONS=--max-old-space-size=4096 vitest run",
    "test:watch": "NODE_OPTIONS=--max-old-space-size=4096 vitest --watch",
    "test:ui": "NODE_OPTIONS=--max-old-space-size=4096 vitest --ui",
    "security:scan": "python3 security-agent/agent.py",
    "security:report": "python3 security-agent/agent.py --format html",
    "security:ci": "python3 security-agent/agent.py --exit-on-critical",
    "v2:dev": "cd v2 && python3 -m http.server 8080",
    "v2:serve": "npx http-server v2 -p 8080 -o",
    "frontend:5555": "vite --config vite.config.5555.ts",
    "frontend:6666": "npx http-server frontends/port-6666 -p 6666 -a 0.0.0.0 --cors",
    "frontend:7777": "vite --config vite.config.7777.ts",
    "frontend:8888": "vite --config vite.config.8888.ts",
    "frontend:9999": "vite --config vite.config.9999.ts",
    "frontends:all": "npm-run-all --parallel frontend:5555 frontend:6666 frontend:7777 frontend:8888 frontend:9999",
    "watchdog": "bash watchdog-frontends.sh",
    "watchdog:start": "bash start-watchdog-background.sh",
    "watchdog:stop": "bash stop-watchdog.sh",
    "watchdog:status": "bash status-watchdog.sh",
    "watchdog:logs": "tail -f logs/watchdog/watchdog-background.log"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^20.0.8",
    "http-server": "^14.1.1",
    "msw": "^2.11.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.2.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.3"
  }
}
