{
  "name": "pokemon-tcg-search",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:6666": "vite --port 6666 --host 0.0.0.0",
    "dev:all": "bash start-dev.sh",
    "dev:all:win": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 vitest",
    "test:run": "NODE_OPTIONS=--max-old-space-size=4096 vitest run",
    "test:watch": "NODE_OPTIONS=--max-old-space-size=4096 vitest --watch",
    "test:ui": "NODE_OPTIONS=--max-old-space-size=4096 vitest --ui",
    "security:scan": "python3 security-agent/agent.py",
    "security:report": "python3 security-agent/agent.py --format html",
    "security:ci": "python3 security-agent/agent.py --exit-on-critical",
    "v2:dev": "cd v2 && python3 -m http.server 9999 --bind 0.0.0.0",
    "v2:serve": "npx http-server v2 -p 9999 -a 0.0.0.0 -o",
    "carousel:dev": "cd carousel && python3 -m http.server 7777 --bind 0.0.0.0",
    "carousel:serve": "npx http-server carousel -p 7777 -a 0.0.0.0 -o",
    "start:all": "concurrently \"npm run dev\" \"npm run v2:serve\" \"npm run carousel:serve\"",
    "start:all+6666": "concurrently \"npm run dev\" \"npm run dev:6666\" \"npm run v2:serve\" \"npm run carousel:serve\""
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^20.0.8",
    "http-server": "^14.1.1",
    "msw": "^2.11.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.2.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.3"
  }
}
