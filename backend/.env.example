# Backend Environment Variables
# Copy this to .env in the backend directory or root

# Google Cloud Vision API
# Set to path of service account JSON key file
GOOGLE_APPLICATION_CREDENTIALS=./gcp-key.json

# Pokemon TCG API Key (existing)
POKEMON_TCG_API_KEY=your-pokemon-tcg-api-key

# Backend Server Configuration
PORT=3001
CORS_ORIGIN=http://localhost:4444

# Frontend OCR API URL (for frontend to connect to backend)
VITE_OCR_API_URL=http://localhost:3001
